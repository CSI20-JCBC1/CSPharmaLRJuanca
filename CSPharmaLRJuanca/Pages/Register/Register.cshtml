@page
@model CSPharmaLRJuanca.Pages.Register.RegisterModel
@{
    ViewData["Title"] = "Register";
}
<div>
    <form method="post" class="d-flex justify-content-center " style="text-align: center">
    <fieldset>
        <legend>Register</legend>
        <div>
            <label for="CodEmpleado">Cod. Empleado:</label>
            <input type="text" name="CodEmpleado" value="" />
        </div>
        <div>
            <label for="ClaveEmpleado">Clave Empleado:</label>
            <input type="password" name="ClaveEmpleado" value="" />
        </div>
        <div>
            <label for="ValContrasenia">Validar Contraseña:</label>
            <input type="password" name="ValContrasenia" value="" />
        </div>

        <div>
            <label>&nbsp;</label>
            <input type="submit" asp-page-handler="Submit" value="submit" class="submit" />
        </div>
        <hr/>
        @Model.Cadena
            
    </fieldset>
</form>

</div>
